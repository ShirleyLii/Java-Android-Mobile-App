<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:baselineAligned="false">

    <fragment
        android:id="@+id/character_fragment"
        android:name="com.example.shirley.cis436project2.CharacterlistFragment"
        class="com.example.shirley.cis436project2.CharacterlistFragment"
        android:layout_width="0dp"
        android:layout_weight="2.0"
        android:layout_height="match_parent"
        tools:layout="@layout/activity_main"/>

    <fragment
        android:id="@+id/rating_fragment"
        android:name="com.example.shirley.cis436project2.WarriorFragment"
        class = "com.example.shirley.cis436project2.WarriorFragment"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:layout_height="match_parent"
        tools:layout="@layout/warriorrating"/>
</LinearLayout>